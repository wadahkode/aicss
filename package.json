{
  "name": "aicss",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "bin": "./dist/cli/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:app": "rollup -c rollup.config.ts --configPlugin typescript",
    "build:bin": "rollup -c rollup.bin.ts --configPlugin typescript",
    "build": "npm run build:app && npm run build:bin",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/chokidar": "^1.7.5",
    "@types/jest": "^29.5.14",
    "chokidar": "^4.0.3",
    "jest": "^29.7.0",
    "rollup": "^3.29.4",
    "ts-jest": "^29.3.0",
    "typescript": "^5.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
